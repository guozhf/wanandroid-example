<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:id="@id/visible" android:drawable="@drawable/q6" />
    <item android:id="@id/masked" android:drawable="@drawable/q5" />
    <transition android:drawable="@drawable/c4" android:toId="@id/visible" android:fromId="@id/masked" />
    <transition android:drawable="@drawable/c5" android:toId="@id/masked" android:fromId="@id/visible" />
</animated-selector>