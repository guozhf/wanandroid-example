<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="2.0dip">
        <shape>
            <solid android:color="#66ebebeb" />
            <corners android:radius="1.5dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="2.0dip">
        <clip>
            <shape>
                <solid android:color="@color/GBK08B" />
                <corners android:radius="1.5dip" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:height="2.0dip">
        <clip>
            <shape>
                <solid android:color="@color/BL01" />
                <corners android:radius="1.5dip" />
            </shape>
        </clip>
    </item>
</layer-list>