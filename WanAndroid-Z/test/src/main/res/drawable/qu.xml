<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/transparent" android:endColor="@color/transparent" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:bottomLeftRadius="@dimen/fn" android:bottomRightRadius="@dimen/fn" />
                <gradient android:startColor="@color/GBL01A" android:endColor="@color/GBL01A" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>