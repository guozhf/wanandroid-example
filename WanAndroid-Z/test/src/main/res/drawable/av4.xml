<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <selector>
            <item android:state_enabled="true" android:drawable="@color/BK09" />
            <item android:state_enabled="false" android:drawable="@color/BK05" />
        </selector>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <selector>
                <item android:state_enabled="true" android:drawable="@color/BL01" />
                <item android:state_enabled="false" android:drawable="@color/BK05" />
            </selector>
        </clip>
    </item>
</layer-list>